{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nmce": {
      "projectType": "library",
      "root": "projects/nmce",
      "sourceRoot": "projects/nmce/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/nmce/tsconfig.lib.json",
            "project": "projects/nmce/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/nmce/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "browsers": ["chrome"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/nmce/tsconfig.lib.json",
              "projects/nmce/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demoapp": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/demoapp",
      "sourceRoot": "projects/demoapp/src",
      "prefix": "app",
      "i18n": {
        "sourceLocale": {
          "baseHref": "/en/",
          "code": "en"
        },
        "locales": {
          "zh-hans": {
            "translation": "locales/messages.zh-Hans.xlf",
            "baseHref": "/zh-hans/"
          },
          "zh-hant": {
            "translation": "locales/messages.zh-Hant.xlf",
            "baseHref": "/zh-hant/"
          },
          "ja": {
            "translation": "locales/messages.ja.xlf",
            "baseHref": "/ja/"
          },
          "es": {
            "translation": "locales/messages.es.xlf",
            "baseHref": "/es/"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/"
            },
            "index": "projects/demoapp/src/index.html",
            "polyfills": [
              "projects/demoapp/src/polyfills.ts"
            ],
            "tsConfig": "projects/demoapp/tsconfig.app.json",
            "aot": true,
            "allowedCommonJsDependencies": [],
            "assets": [
              "projects/demoapp/src/favicon.ico",
              "projects/demoapp/src/assets",
              "projects/demoapp/src/print190826.css",
              {
								"glob": "**/*",
								"input": "node_modules/@kolkov/angular-editor/assets/icons",
								"output": "assets/ae-icons/"
							}
            ],
            "styles": [
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/demoapp/src/styles.css",
							"node_modules/@kolkov/angular-editor/themes/default.scss"
            ],
            "scripts": [],
            "browser": "projects/demoapp/src/main.ts"
          },
          "configurations": {
            "production": {
              "localize": true,
              "fileReplacements": [
                {
                  "replace": "projects/demoapp/src/environments/environment.ts",
                  "with": "projects/demoapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputPath": {
                "base": "appdist/prod/"
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {
              "outputPath": {
                "base": "appdist/development/"
              },
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "demoapp:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "demoapp:build:production"
            },
            "development": {
              "buildTarget": "demoapp:build:development",
              "prebundle": {
                "exclude": [
                  "nmce",
                  "nmce-func",
                  "nmce-directives",
                  "nmce-pipes",
                  "nmce-json-diff",
                  "nmce-html-editor"
                ]
              }
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "ng-extract-i18n-merge:ng-extract-i18n-merge",
          "options": {
            "buildTarget": "demoapp:build",
            "format": "xliff",
            "includeContext": "sourceFileOnly",
            "outputPath": "locales",
            "targetFiles": [
              "messages.zh-Hans.xlf",
              "messages.zh-Hant.xlf",
              "messages.ja.xlf",
              "messages.es.xlf"
            ],
            "sourceFile": "messages.xlf"
          }
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "browsers": ["chrome"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demoapp/tsconfig.app.json",
              "projects/demoapp/tsconfig.spec.json",
              "projects/demoapp/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/demoapp/e2e/protractor.conf.js",
            "devServerTarget": "demoapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demoapp:serve:production"
            }
          }
        }
      }
    },
    "nmce-html-editor": {
      "projectType": "library",
